{
  "name": "Source Pilot",
  "version": "1.0.0",
  "description": "IDE like file navigation, for GitHub",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "out/production/source-pilot/lib/kotlin.js",
        "out/production/source-pilot/source-pilot.js"
      ],
      "runat": "document_end"
    }
  ]
}